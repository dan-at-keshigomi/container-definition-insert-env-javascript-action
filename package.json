{
  "name": "container-definition-insert-env-javascript-action",
  "version": "1.0.0",
  "description": "Replaces the Environment section of ContainerDefinition with the specified variables",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "postbuild": "cp package.json dist/package.json && cp package-lock.json dist/package-lock.json && cd dist && npm ci --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-at-keshigomi/container-definition-insert-env-javascript-action.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dan-at-keshigomi/container-definition-insert-env-javascript-action/issues"
  },
  "homepage": "https://github.com/dan-at-keshigomi/container-definition-insert-env-javascript-action#readme",
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.unit.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "jsonpath": "^1.1.1",
    "yaml-cfn": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonpath": "^0.2.0",
    "@types/node": "^16.10.1",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
